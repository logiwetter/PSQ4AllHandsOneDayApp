										<Division
											Class="panel-heading"
											HtmlDiv="True"
											ID="panel-heading"
											>
											<Division
												Class="pull-right"
												HtmlDiv="True"
												ID="pull-right"
												>
												<Division
													Class="btn btn-default btn-xs add-inactive-link"
													HtmlDiv="False"
													ID="add-inactive-item"
													>
													<Label
														Class="fa fa-plus text-success"
														ID="add-item"
													/>
												</Division>
											</Division>
											<Label
												Caption="Inactive Items"
											/>
										</Division>
										<Division
											Class="panel-body"
											HtmlDiv="True"
											ID="panel-body"
											>
											<Division
												HtmlDiv="True"
												ID="InactiveMenu"
												>
												<Remark>
													<IncludeHtml
														Html="&lt;ul id=&quot;InactiveMenuItems&quot; class=&quot;MenuBuilder&quot;&gt;&lt;/ul&gt;"
													/>
												</Remark>
												<RepeatElements
													ID="BuildInactiveMenu"
													>
													<DataLayer
														ID="dlMenuInactiveXML"
														RemoveNamespace="True"
														Type="XMLFile"
														XMLFile="@Function.AppPhysicalPath~\_SupportFiles\_theme_files.Menu.xml"
														>
														<CalculatedColumn
															Formula="decodeURI(&quot;@Data.inactive~&quot;)"
															ID="MenuInactive"
														/>
														<RemoveColumns
															ColumnNames="active,inactive"
															ID="RemoveEncoded"
														/>
														<IfDataError
															ID="NoInactiveMenu"
															>
															<DataLayer
																ID="dlEmptyInactiveMenu"
																Type="Static"
																>
																<StaticDataRow
																	MenuInactive=""
																/>
															</DataLayer>
														</IfDataError>
													</DataLayer>
													<Label
														Caption="@Repeat.MenuInactive~"
														Format="HTML"
													/>
												</RepeatElements>
											</Division>
										</Division>
									</Division>
								</ResponsiveColumn>
							</ResponsiveRow>
							<IncludeScript
								ID="InitAdminControls"
								IncludedScript="AdminControls()"
							/>
						</Division>
					</Division>
				</ResponsiveColumn>
			</ResponsiveRow>
			<Remark>
				<Division
					HtmlDiv="True"
					ID="divControls"
					>
					<Label
						Caption="&lt;i id=&quot;icon-save&quot; class=&quot;fa fa-save&quot;&gt;&lt;/i&gt; Save"
						Class="btn btn-primary btn-save"
						Format="HTML"
						ID="btn-save"
					/>
					<Spaces/>
					<Label
						Caption="&lt;i class=&quot;fa fa-remove&quot;&gt;&lt;/i&gt; Clear and Reset Menu"
						Class="btn btn-default btn-reset"
						Format="HTML"
						ID="btn-reset"
					/>
				</Division>
			</Remark>
			<InputTextArea
				Class="hidden"
				ID="MenuActiveOutput"
			/>
			<InputTextArea
				Class="hidden"
				ID="MenuInactiveOutput"
			/>
		</Division>
		<Division
			Condition="&quot;@Request.Export~&quot;==&quot;Menu&quot;"
			HtmlDiv="True"
			ID="ExportMenu"
			ShowModes="rdExport"
			>
			<DataTable
				ID="Menu"
				>
				<DataLayer
					ID="dlMenuExport"
					Type="Static"
					>
					<StaticDataRow
						active="@Request.MenuActiveOutput~"
						inactive="@Request.MenuInactiveOutput~"
					/>
				</DataLayer>
			</DataTable>
		</Division>
	</Body>
	<ideTestParams
		Export=""
		MenuActiveOutput=""
		MenuInactiveOutput=""
	/>
</Report>
