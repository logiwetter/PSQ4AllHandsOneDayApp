															Formula="if(!&apos;@Data.DefinitionType~&apos;){
	&apos;rdPage.aspx?rdReport=@Data.rdReport~&amp;rdLoadBookmark=True&amp;rdBookmarkCollection=@Data.BookmarkCollection~&amp;rdBookmarkUserName=@Data.BookmarkUserName~&amp;rdBookmarkID=@Data.BookmarkID~&amp;rdSharedBookmarkID=&amp;goBookmarkCaption=&apos;+escape(&apos;@Data.Caption~&apos;);
}else if(&apos;@Data.DefinitionType~&apos;){
	&apos;rdPage.aspx?rdReport=@Data.ID~&apos;;
}"
