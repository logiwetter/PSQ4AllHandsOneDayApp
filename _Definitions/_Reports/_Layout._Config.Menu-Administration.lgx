	&apos;@Data.BookmarkID~&apos;.replace(/[^a-zA-Z0-9]/g,&apos;&apos;);
}else if(&apos;@Data.ID~&apos;){
	&apos;@Data.ID~&apos;.replace(/[^a-zA-Z0-9]/g,&apos;&apos;);
