<?xml version="1.0" encoding="utf-8"?>
<Report
	ID="_Layout._Config.Menu-Administration"
	SecurityReportRightID="Admin"
	>
	<LocalData>
		<DataLayer
			ID="RawMenuXML"
			RemoveNamespace="True"
			Type="XMLFile"
			XMLFile="@Function.AppPhysicalPath~\_SupportFiles\_theme_files.Menu.xml"
			>
			<IfDataError
				ID="NoMenu"
				>
				<DataLayer
					ID="dlEmptyMenu"
					Type="Static"
					>
					<StaticDataRow
						active="Empty"
					/>
				</DataLayer>
			</IfDataError>
			<DataLayerLink
				ID="dlRawMenuXML"
			/>
		</DataLayer>
	</LocalData>
